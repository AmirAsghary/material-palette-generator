{
  "name": "material-palette-generator",
  "version": "1.0.0",
  "description": "Material design palette generator",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/eugeneford/material-palette-generator.git"
  },
  "scripts": {
