    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
